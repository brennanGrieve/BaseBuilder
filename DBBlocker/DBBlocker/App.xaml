<Application x:Class="DBBlocker.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="Graphics" Source="Shapes\Graphics.xaml"/>
                <ResourceDictionary x:Name="Styles" Source="Styles.xaml"/>
                <ResourceDictionary x:Name="Bindings" Source="DataBindings.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
    
</Application>
