<local:InitialBlockBase x:Class="DBBlocker.SelectBlockWildCard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DBBlocker"
             mc:Ignorable="d" Height="95" Width="150">
    <Grid Margin="0,0,-25,0">
        <local:InitialBlock Fill="#FFB21D1D" Stroke="Black"/>
        <TextBlock Name="SelectWildCardText" Margin="0,-25,25,0" FontWeight="Bold" Height="26" TextWrapping="Wrap" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="Black" FontSize="16" Text="SELECT *"/>
    </Grid>
</local:InitialBlockBase>
