<local:QueryBlockBase x:Class="DBBlocker.BetweenBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DBBlocker"
             Height="95" Width="150">
    <Grid Margin="0,0,-25,0">
        <local:SubBlock Fill="{StaticResource TurqoiseBlockFill}" Stroke="Black"/>
        <TextBlock Foreground="Black" HorizontalAlignment="Center" FontWeight="Bold" Margin="-5,-3,0,0" TextWrapping="Wrap" FontSize="16" Text="BETWEEN" VerticalAlignment="Top"/>
        <TextBox IsEnabled="False" HorizontalAlignment="Left" Height="19" Margin="28,16,0,0" TextWrapping="Wrap" FontSize="12" VerticalAlignment="Top" Width="120" Text="[VALUE_1]"/>
        <TextBlock Foreground="Black" HorizontalAlignment="Center" FontWeight="Bold" Margin="-5,32,0,0" TextWrapping="Wrap" FontSize="16" Text="AND" VerticalAlignment="Top"/>
        <TextBox IsEnabled="False" HorizontalAlignment="Left" Height="19" Margin="28,52,0,0" TextWrapping="Wrap" FontSize="12" VerticalAlignment="Top" Width="120" Text="[VALUE_2]"/></Grid>
</local:QueryBlockBase>
