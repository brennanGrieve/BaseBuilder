<local:QueryBlockBase x:Class="DBBlocker.WhereNotBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DBBlocker"
             Height="95" Width="150">
    <Grid Margin="0,0,-25,0">
        <local:SubBlock Fill="#FF910CCF" Stroke="Black"/>
        <TextBlock HorizontalAlignment="Center" FontWeight="Bold" TextWrapping="Wrap" VerticalAlignment="Top" Margin="0,0,0,0" Text="WHERE NOT" FontSize="16"></TextBlock>
        <TextBox HorizontalAlignment="Left" Height="32" Margin="30,21,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="130" IsEnabled="False"/>
    </Grid>
</local:QueryBlockBase>
