<UserControl x:Class="DBBlocker.OnBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DBBlocker"
             mc:Ignorable="d" 
             Height="75" Width="175">
    <Grid>
        <local:SubBlock Fill="#FFA03884" Stroke="Black"/>
        <TextBlock HorizontalAlignment="Left" Margin="74,-1,0,0" TextWrapping="Wrap" FontSize="18" Text="On" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="28" Margin="27,22,0,0" TextWrapping="Wrap" FontSize="18" VerticalAlignment="Top" Width="139"/>
    </Grid>
</UserControl>
