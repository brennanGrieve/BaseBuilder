<local:QueryBlockBase x:Class="DBBlocker.OnBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DBBlocker"
             mc:Ignorable="d" 
             Height="95" Width="150">
    <Grid Margin="0,0,-25,0">
        <local:SubBlock Fill="#FFA03884" Stroke="Black"/>
        <TextBlock HorizontalAlignment="Left" FontWeight="Bold" Margin="74,-1,0,0" TextWrapping="Wrap" FontSize="16" Text="ON" VerticalAlignment="Top"/>
        <TextBox IsEnabled="False" HorizontalAlignment="Left" Height="32" Margin="30,21,0,0" TextWrapping="Wrap" FontSize="20" VerticalAlignment="Top" Width="130"/>
    </Grid>
</local:QueryBlockBase>
