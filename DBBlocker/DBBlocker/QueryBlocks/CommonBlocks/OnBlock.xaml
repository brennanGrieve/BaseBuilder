<local:QueryBlockBase x:Class="DBBlocker.OnBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DBBlocker"
             Height="95" Width="150"
             AllowDrop="False">
    <Grid Margin="0,0,-25,0">
        <local:SubBlock Fill="{StaticResource PlumBlockFill}" Stroke="Black"/>
        <local:SingleBoxBlockTextBlock Text="ON"/>
        <local:SingleBoxBlockTextBox />
    </Grid>
</local:QueryBlockBase>
