<local:QueryBlockBase x:Class="DBBlocker.InNestBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DBBlocker"
             Height="95" Width="135"
             AllowDrop="False">
    <Grid Margin="0,0,-35,0">
        <local:NestableBlock Fill="{StaticResource DeepBlueBlockFill}" Stroke="Black"></local:NestableBlock>
        <TextBlock HorizontalAlignment="Center" FontWeight="Bold" Height="27" Margin="25,20,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="66" Foreground="Black" FontSize="20" Text="IN"></TextBlock>
    </Grid>
</local:QueryBlockBase>
