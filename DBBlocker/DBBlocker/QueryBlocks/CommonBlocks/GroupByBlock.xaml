<local:QueryBlockBase x:Class="DBBlocker.GroupByBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DBBlocker"
             Height="95" Width="150">
    <Grid Margin="0,0,-25,0">
        <local:SubBlock Fill="{StaticResource GreenBlockFill}" Stroke="Black"></local:SubBlock>
        <local:SingleBoxBlockTextBlock Text="GROUP BY"/>
        <local:SingleBoxBlockTextBox Text="Grouping Value"/>
    </Grid>
</local:QueryBlockBase>
