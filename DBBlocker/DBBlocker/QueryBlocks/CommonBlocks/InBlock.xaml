<local:QueryBlockBase x:Class="DBBlocker.InBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DBBlocker"
             Height="95" Width="150">
    <Grid Margin="0,0,-25,0">
        <local:SubBlock Fill="#FF3636BB" Stroke="Black"/>
        <TextBlock FontWeight="Bold" Height="22" HorizontalAlignment="Center" Margin="-10,0,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="Black" FontSize="16" Text="IN"/>
        <TextBox IsEnabled="False" Text="(VAL1, VAL2, VAL3)" HorizontalAlignment="Left" Height="22" Margin="30,25,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="130" FontSize="14"/>
    </Grid>
</local:QueryBlockBase>
