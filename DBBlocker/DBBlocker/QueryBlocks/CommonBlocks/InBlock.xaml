<local:QueryBlockBase x:Class="DBBlocker.InBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DBBlocker"
             Height="95" Width="150"
             AllowDrop="False">
    <Grid Margin="0,0,-25,0">
        <local:SubBlock Fill="{StaticResource DeepBlueBlockFill}" Stroke="Black"/>
        <local:SingleBoxBlockTextBlock Text="IN"/>
        <local:SingleBoxBlockTextBox Text="(VAL1, VAL2, VAL3)"/>
    </Grid>
</local:QueryBlockBase>
