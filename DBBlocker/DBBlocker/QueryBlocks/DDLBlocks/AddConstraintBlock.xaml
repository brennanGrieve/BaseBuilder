<local:QueryBlockBase x:Class="DBBlocker.AddConstraintBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DBBlocker"
             Height="95" Width="150">
    <Grid Margin="0,0,-25,0">
        <local:SubBlock Stroke="Black" Fill="#FF3FA229"/>
        <TextBlock HorizontalAlignment="Center" FontWeight="Bold" Margin="-10,0,0,0" TextWrapping="Wrap" FontSize="14" Text="ADD CONSTRAINT" TextAlignment="Center" VerticalAlignment="Top"/>
        <TextBox IsEnabled="False" HorizontalAlignment="Left" Height="32" Margin="30,21,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="130" FontSize="20"/>
    </Grid>
</local:QueryBlockBase>
