<local:QueryBlockBase x:Class="DBBlocker.DropColumnBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DBBlocker"
             Height="95" Width="150">
    <Grid Margin="0,0,-25,0">
        <local:SubBlock Fill="#FF910CCF" Stroke="Black"/>
        <TextBlock Foreground="Black" HorizontalAlignment="Center" FontWeight="Bold" Margin="-10,0,0,0" TextWrapping="Wrap" FontSize="16" Text="DROP COLUMN" TextAlignment="Center" VerticalAlignment="Top"/>
        <TextBox IsEnabled="False" HorizontalAlignment="Left" Height="32" Margin="30,21,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="130" FontSize="20"/>
    </Grid>
</local:QueryBlockBase>
