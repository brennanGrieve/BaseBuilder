<local:InitialBlockBase x:Class="DBBlocker.AlterTableBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DBBlocker"
             Height="95" Width="150"
             AllowDrop="False">
    <Grid Margin="0,0,-25,0">
        <local:InitialBlock Fill="{StaticResource InitialBlockFill}" Stroke="Black"/>
        <local:SingleBoxBlockTextBlock Text="ALTER TABLE"/>
        <local:SingleBoxInitialBlockTextBox/>
    </Grid>
</local:InitialBlockBase>
