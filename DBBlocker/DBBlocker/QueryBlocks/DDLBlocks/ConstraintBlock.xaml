<local:QueryBlockBase x:Class="DBBlocker.ConstraintBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DBBlocker"
             Height="95" Width="150"
             AllowDrop="False">
    <Grid Margin="0,0,-25,0">
        <local:SubBlock Fill="{StaticResource PlumBlockFill}" Stroke="Black"/>
        <ComboBox IsEnabled="False" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="28,26,0,0" SelectedIndex="0">
            <ComboBoxItem Content="PRIMARY KEY"/>
            <ComboBoxItem Content="FOREIGN KEY"/>
            <ComboBoxItem Content="UNIQUE"/>
            <ComboBoxItem Content="NOT NULL"/>
        </ComboBox>

    </Grid>
</local:QueryBlockBase>
