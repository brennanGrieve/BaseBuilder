<UserControl x:Class="DBBlocker.InsertBlockTextBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DBBlocker"
             mc:Ignorable="d" 
             d:DesignHeight="75" d:DesignWidth="175">
    <Grid>
        <local:InitialBlock Fill="#FFB21D1D" Stroke="Black"/>
        <TextBox IsEnabled="False" HorizontalAlignment="Left" Height="31" TextWrapping="Wrap" Text="*" VerticalAlignment="Top" Width="86" Margin="75,21,0,0" FontSize="20"/>
    </Grid>
</UserControl>
