<UserControl x:Class="DBBlocker.InsertBlockBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DBBlocker"
             Height="95" Width="150">
    <Grid Margin="0,0,-25,0">
        <local:InitialBlock Fill="#FFB21D1D" Stroke="Black"/>
        <TextBlock HorizontalAlignment="Center" FontWeight="Bold" Margin="-25,0,0,0" TextWrapping="Wrap" FontSize="16" Text="INSERT INTO" VerticalAlignment="Top"/>
        <TextBox IsEnabled="False" HorizontalAlignment="Left"  Margin="5,21,0,0" TextWrapping="Wrap" Text="" FontSize="20" VerticalAlignment="Top" Height="32" Width="155"/>

    </Grid>
</UserControl>
