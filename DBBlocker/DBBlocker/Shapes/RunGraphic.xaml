<UserControl x:Class="DBBlocker.RunGraphic"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DBBlocker"
             Height="70" Width="90">
    <Grid Background="Green">
        <Border BorderBrush="Black" BorderThickness="5"/>
        <Path Stroke="Black" StrokeThickness="20" StrokeEndLineCap="Triangle">
            <Path.Data>
                <LineGeometry StartPoint="15,35" EndPoint="60, 35"/>
            </Path.Data>
        </Path>
        <Path Stroke="Black" StrokeThickness="10" StrokeEndLineCap="Triangle" StrokeStartLineCap="Triangle">
            <Path.Data>
                <LineGeometry StartPoint="50,15" EndPoint="70, 35"/>
            </Path.Data>
        </Path>
        <Path Stroke="Black" StrokeThickness="10" StrokeEndLineCap="Triangle" StrokeStartLineCap="Triangle">
            <Path.Data>
                <LineGeometry StartPoint="50,55" EndPoint="70, 35" />
            </Path.Data>
        </Path>

    </Grid>
</UserControl>
